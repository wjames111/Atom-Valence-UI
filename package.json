{
  "configSchema": {
  "colors": {
    "order": 1,
    "type": "object",
    "title": "Colors",
    "properties": {
      "abaseColor": {
        "title": "Primary color",
        "description": "Changes the main theme color.",
        "type": "color",
        "default": "#009688"
      },
      "accentColor": {
        "title": "Secondary color",
        "description": "This color is used as an accent to the Primary Color.",
        "type": "color",
        "default": "#FFFFFF"
      },

      "toggleTabBar": {
        "title": "Toggle Tab-Bar",
        "description": "toggles tab-bar on hover ",
        "type": "boolean",
        "default": true
      },
      "directoryIndicator": {
        "title": "Directory Indicator",
        "description": "highlight directory hierarchy",
        "type": "boolean",
        "default": true
      },
      "matchSyntaxTheme": {
        "title": "Match Syntax Theme",
        "description": "override syntax theme's background color to match current theme",
        "type": "boolean",
        "default": true
      },
      "fontSize": {
        "title": "Change Font-size",
        "description": "adjust font-size for all UI components. For editor panel adjustments go to editor settings",
        "type": "integer",
        "default": "12"
      }
    }
  }
},


  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0"
  },
  "main": "./lib/main",
  "name": "wills-theme-ui",
  "theme": "ui",
  "version": "0.0.0",
  "description": "A short description of your UI theme",
  "keywords": [
    "ui",
    "theme"
  ],
  "repository": "https://github.com/atom-community/ui-theme-template",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "fs": "0.0.1-security"
  }
}
